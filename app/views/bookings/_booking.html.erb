<div id="<%= dom_id booking %>">
  <p>
    <strong>Accepted:</strong>
    <%= booking.accepted %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= booking.status %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= booking.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= booking.end_date %>
  </p>

  <p>
    <strong>Comment:</strong>
    <%= booking.comment %>
  </p>

  <p>
    <strong>User:</strong>
    <%= booking.user_id %>
  </p>

  <p>
    <strong>Flat:</strong>
    <%= booking.flat_id %>
  </p>

</div>
